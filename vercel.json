{
    "redirects": [
        {
            "source": "https://creator-toolkit.com/:path*",
            "destination": "https://www.creator-toolkit.com/:path*",
            "permanent": true
        },
        {
            "source": "http://creator-toolkit.com/:path*",
            "destination": "https://www.creator-toolkit.com/:path*",
            "permanent": true
        },
        {
            "source": "http://www.creator-toolkit.com/:path*",
            "destination": "https://www.creator-toolkit.com/:path*",
            "permanent": true
        },
        {
            "source": "https://creator-toolkit.com",
            "destination": "https://www.creator-toolkit.com",
            "permanent": true
        },
        {
            "source": "http://creator-toolkit.com",
            "destination": "https://www.creator-toolkit.com",
            "permanent": true
        },
        {
            "source": "http://www.creator-toolkit.com",
            "destination": "https://www.creator-toolkit.com",
            "permanent": true
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Strict-Transport-Security",
                    "value": "max-age=31536000; includeSubDomains; preload"
                },
                {
                    "key": "Content-Security-Policy",
                    "value": "default-src 'self'; script-src 'self'; style-src 'self' 'unsafe-inline'; font-src 'self'; img-src 'self' data:; connect-src 'self'"
                },
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "Referrer-Policy",
                    "value": "strict-origin-when-cross-origin"
                }
            ]
        }
    ]
}
